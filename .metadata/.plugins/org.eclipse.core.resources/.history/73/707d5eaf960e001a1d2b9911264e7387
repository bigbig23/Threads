package demo1;

public class App {

	public static void main(String[] args) {
 
		Runner runner = new Runner();
		
		Thread t1 = new Thread(new Runnable() {

			@Override
			public void run() {
				runner.firstThread();
			}
		});
		
		
		Thread t2 = new Thread(new Runnable() {

			@Override
			public void run() {
				runner.secondThread();
			}
		});
	}

}
